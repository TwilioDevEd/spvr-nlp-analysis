{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "arrow-parens": [2, "always"],
    "function-paren-newline": "off",
    "implicit-arrow-linebreak": "off",
    "react/jsx-filename-extension": "off"
  }
}
